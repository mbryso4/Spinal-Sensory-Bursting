                    duration3 = 0;
                    duration_second3 = 0;
                    rectint3 = 0;
                    app.bigpeaks3 = 0;
                    app.biglocs3 = 0;
                    app.firstloc3 = 0;
                    app.lastloc3 = 0;
                    app.firstloc3 = 0;
                    meanduration(3) = 0;
                    stdduration(3) = 0;
                    meandurationseconds(3) = 0;
                    stddurationseconds(3) = 0;
                    meanamplitude(3) = 0;
                    stdamplitude(3) = 0;
                    meanrectint(3) = 0;
                    stdrectint(3) = 0;
                    numbursts(3) = 0;
                    numspikes(3) = 0;
